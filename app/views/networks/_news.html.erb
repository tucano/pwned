<!-- NEWS SUBCONTENT CELL -->
<div class="corner-subcontent-top"></div>                        
<div class="subcontent-box">
  <h1 class="webtemplate">News</h1>                  
  <h2><%= link_to "RSS last 5 feeds", formatted_networks_url(:rss) %></h2> 
  <ul>
  <% @networks.last(5).each do |n| %>
    <li><%= link_to n.name, formatted_network_url(n.id, :rss) %></li>
  <% end %>
  </ul>
</div>  
<div class="corner-subcontent-bottom"></div>