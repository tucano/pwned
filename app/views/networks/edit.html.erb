<h1>Editing network</h1>

<% form_for(@network) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :edgefile %><br />
    <%= f.text_field :edgefile %>
  </p>
  <p>
    <%= f.label :annotationfile %><br />
    <%= f.text_field :annotationfile %>
  </p>
  <p>
    <%= f.label :config %><br />
    <%= f.text_field :config %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @network %> |
<%= link_to 'Back', networks_path %>
